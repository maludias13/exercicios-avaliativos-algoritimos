#include <iostream>

using namespace std;
void divisivel (int n){
   
    if(n%2==0){
        cout<<n<<" é divisível por 2"<<endl;
        }else{
           cout<<n<<" não é divisível por 2"<<endl;
            }
            
    if(n%3==0){
        cout<<n<<" é divisível por 3"<<endl;
        } else {
            cout<<n<<" não é divisível por 3"<<endl;
            }
    if(n%5==0){
        cout<<n<<" é divisível por 5"<<endl;
        } else {
            cout<<n<<" não é divisível por 5"<<endl;
            }
    if(n%7==0){
     cout<<n<<" é divisível por 7"<<endl;
     } else {
            cout<<n<<" não é divisível por 7"<<endl;
    }
}
int main()
{   int n;
    cin>>n;
    divisivel(n);

    return 0;
}
